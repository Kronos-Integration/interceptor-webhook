{
  "name": "@kronos-integration/interceptor-webhook",
  "version": "0.0.0-semantic-release",
  "sideEffects": false,
  "type": "module",
  "main": "./src/webhook.mjs",
  "exports": {
    ".": "./src/webhook.mjs"
  },
  "description": "interceptor for web hooks",
  "keywords": [
    "kronos-interceptor"
  ],
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme ./src/webhook.mjs --section=API",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint ./src/webhook.mjs"
  },
  "dependencies": {
    "@kronos-integration/interceptor": "^10.0.6"
  },
  "devDependencies": {
    "ava": "^3.13.0",
    "c8": "^7.3.1",
    "documentation": "^13.0.2",
    "semantic-release": "^17.1.2"
  },
  "engines": {
    "node": ">=14.13.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/interceptor-webhook.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/interceptor-webhook/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/interceptor-webhook#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-github",
      "arlac77/template-kronos-component"
    ]
  }
}
