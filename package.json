{
  "name": "@kronos-integration/interceptor-webhook",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./src/webhook.mjs"
  },
  "description": "interceptor for web hooks",
  "keywords": [
    "kronos-interceptor"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 4m tests/*-ava.mjs tests/*-ava-node.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 4m tests/*-ava.mjs tests/*-ava-node.mjs && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme --section=API ./src/**/*.mjs",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint ./src/**/*.mjs"
  },
  "dependencies": {
    "@kronos-integration/interceptor": "^10.2.37",
    "model-attributes": "^4.2.2"
  },
  "devDependencies": {
    "@kronos-integration/endpoint": "^9.4.44",
    "ava": "^5.3.1",
    "c8": "^8.0.1",
    "documentation": "^14.0.2",
    "semantic-release": "^22.0.7"
  },
  "engines": {
    "node": ">=20.9.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/interceptor-webhook"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/interceptor-webhook/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/interceptor-webhook#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-kronos-component"
    ]
  }
}
